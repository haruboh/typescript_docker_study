{
    "compilerOptions": {
      "module": "commonjs",
      "target": "es2020",
      "sourceMap": true,
      "outDir": "./html/js/dist",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "types": [
        "node"
      ]
    },
    "include": [
      "html/js/ts/**/*"
    ],
    "exclude": [
      "node_modules"
    ]
  }